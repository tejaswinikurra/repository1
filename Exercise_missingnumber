public class Exercise {
   static int findMissingNumberInArray(int[] arr) {
    int n = 6;
   int a = (n*(n+1))/2;
    int sum = 0;
    for(int i=0; i<arr.length; i++)
    {
        sum += arr[i];
    }
    return a-sum;
   }
   public static void main(String[]args)
   {
       int n = 6;
       int[] array = {1, 2, 3, 4, 6};
       int ele = Exercise.findMissingNumberInArray(array);
       System.out.println(ele);
   }
}
